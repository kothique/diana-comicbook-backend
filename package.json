{
  "name": "diana-comicbook-backend",
  "version": "1.0.0",
  "description": "My Moleculer microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "cli": "moleculer connect ",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "moleculer-repl": "^0.5.3"
  },
  "dependencies": {
    "async-busboy": "^0.7.0",
    "moleculer": "^0.13.0",
    "moleculer-web": "^0.8.0",
    "msgpack-lite": "^0.1.26"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
